#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/frontend/node_modules/acorn/bin/node_modules:/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/frontend/node_modules/acorn/node_modules:/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/frontend/node_modules:/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/node_modules:/proc/cygdrive/c/Users/admin/Downloads/node_modules:/proc/cygdrive/c/Users/admin/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/frontend/node_modules/acorn/bin/node_modules:/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/frontend/node_modules/acorn/node_modules:/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/frontend/node_modules:/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/node_modules:/proc/cygdrive/c/Users/admin/Downloads/node_modules:/proc/cygdrive/c/Users/admin/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/admin/Downloads/shradha-kapoor/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
